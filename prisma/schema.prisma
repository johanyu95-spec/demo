generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model prescriptions {
  id                BigInt
  code              String
  name              String
  dear              String
  letter            String
  concept           String
  movie             String
  challengeConcept  String
  challengeKeyword  String
  challengeActivity String
  maintainConcept   String
  maintainKeyword   String
  maintainActivity  String
  reconcileConcept  String
  reconcileKeyword  String
  reconcileActivity String

  @@id([id, code, name, dear, letter, concept, movie, challengeConcept, challengeKeyword, challengeActivity, maintainConcept, maintainKeyword, maintainActivity, reconcileConcept, reconcileKeyword, reconcileActivity])
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model results {
  id         BigInt   @id @unique @default(autoincrement())
  created_at DateTime @db.Timestamptz(6)
  updated_at DateTime @updatedAt @db.Timestamptz(6)
  answers    Json     @db.Json
  code       String
  nickname   String
  comment    String
}
